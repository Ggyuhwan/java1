package ch05_control;

import java.util.Iterator;

public class LoopFor {

	public static void main(String[] args) {
		// 콘솔창에 1~10 까지 출력하시오!
		
		int num = 1;
		System.out.println(num++);
		System.out.println(num++);
		System.out.println(num++);
		System.out.println(num++);
		System.out.println(num++);
		
		//for 문
		for(int i = 1; i <= 10; i++) {
			System.out.println(i);
		}
	}

}
