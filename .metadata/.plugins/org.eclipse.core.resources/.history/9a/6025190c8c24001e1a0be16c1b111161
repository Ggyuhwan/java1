package ch04_Operator;

import java.util.Iterator;
import java.util.Scanner;

public class _01_If {

	public static void main(String[] args) {
		
		
		
		for (int i = 0 ; i < 10 ; i++) {
			System.out.println(i+i);
		}
		
		for (int i = 0 ; i < 5 ; i+=2) {
			System.out.println("드루와"+i);
		}

		for (int i = 5; i > 0; i-- ) {
			System.out.println(i);
		}
		
	}

}
