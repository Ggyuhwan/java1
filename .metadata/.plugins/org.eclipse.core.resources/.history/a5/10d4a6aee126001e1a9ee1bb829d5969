package practice.submit01;

import java.util.Iterator;

public class q0404 {

	public static void main(String[] args) {
		
		String star = "*";
		for(int i=1; i<=4; i++) {
			star+=i;
			System.out.println(star);
			for(int j=1; j<=i; j++) {
				System.out.println("*");
			}System.out.println("");
		}
			
		
	}

}
