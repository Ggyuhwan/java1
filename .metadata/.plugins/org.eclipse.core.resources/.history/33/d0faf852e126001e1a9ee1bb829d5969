package practice.submit01;

import java.util.Iterator;

public class q0404 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String star = "*";
		
		
		for(int i=1; i<5; i++) {
			star += " ";
			System.out.println(star);
			
			for(int j=1; j<i; j++) {
				star += "1";
				System.out.println(star);
			}
			
		}
	}

}
