package ch04_Operator;

import java.util.Scanner;

public class _01_If {

	public static void main(String[] args) {
		
		//swith case
		int ranking =1;
		if (ranking == 1) {
			System.out.println("전액 장학금");
		}else if (ranking == 2) {
			System.out.println("반액 장학금");
		}else if (ranking == 3) {
			System.out.println("반액 장학금");
		}else {
			System.out.println("대상 아님");
		}
		System.out.println("조회 완료1");
	}

}
