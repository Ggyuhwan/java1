package ch04_Operator;

import java.util.Scanner;

public class _01_If {

	public static void main(String[] args) {
		
		boolean han = false;
		boolean man = false;
		boolean oran = true;
		
		if(han) {
			System.out.println("한라봉+1");
		}else if (man) {
			System.out.println("망고+1");		
		}
		else if (oran) {
			System.out.println("오렌지+1");
			
		}else {
			System.out.println("아아+1");
		}
		System.out.println("주문완료1");
	}

}
