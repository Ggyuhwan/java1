package ch04_Operator;

import java.util.Scanner;

public class _01_If {

	public static void main(String[] args) {
		
		
		
		int grade = 2;
		int price = 7000;
		switch (grade) {
		case 1:
			price += 1000;
		case 2:
			price += 1000;
		case 3:
			price += 1000;
			break;
		}
		System.out.println(grade + "등급제품의 가격:"+price+"원");
		
	}

}
