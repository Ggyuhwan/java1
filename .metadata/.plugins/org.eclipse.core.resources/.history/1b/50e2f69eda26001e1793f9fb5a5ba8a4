package practice.submit01;

import java.util.Iterator;

public class q0404 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String star = "**";
		
		for(int i=0; i<=5; i++) {
			
			for(int j=0; j<=i; j++) {
				
				System.out.println("*");
			}
			System.out.println("");
		}
	}

}
