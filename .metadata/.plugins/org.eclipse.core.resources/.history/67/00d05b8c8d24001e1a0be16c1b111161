package ch04_Operator;

import java.util.Iterator;
import java.util.Scanner;

public class _01_If {

	public static void main(String[] args) {
		
		int d = 25;
		int m = 0;
		while (m < d) {
			System.out.println("발차기합니다");
			System.out.println("현재이동거리:"+m);
			m += 3;
		}
		System.out.println("도착");
	}

}
